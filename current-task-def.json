        },
        {
            "name": "RPC_URL_ETHEREUM_SEPOLIA",
            "value": "https://ethereum-sepolia-rpc.publicnode.com"
        }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "secrets": [
        {
            "name": "EVM_PRIVATE_KEY",
